<?xml version="1.0"?>

<!--
	NOTE:
	This file is not actually a part of Corrosion.  It has been included in order
	to get it to build with Catkin, since Catkin won't build subdirectories unless
	it has a `package.xml` file.
-->
<package format="2">
	<name>corrosion</name>
	<version>0.0.1</version>
	<description>
		A tool for integrating Rust into an existing CMake project.
	</description>
	<license>MIT</license>
	<maintainer email="andrew.gaspar@outlook.com">Andrew Gaspar</maintainer>

	<export>
		<build_type>cmake</build_type>
	</export>
</package>
